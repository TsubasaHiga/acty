{
  "name": "acty",
  "type": "module",
  "version": "0.0.5",
  "private": true,
  "scripts": {
    "dev": "astro dev & yarn convertImages --watch",
    "start": "astro dev",
    "build": "yarn convertImages && astro check && tsc --noEmit && astro build",
    "preview": "yarn build && astro preview",
    "archive": "yarn build && yarn node --loader ts-node/esm utils/genDistArchive.ts",
    "astro": "astro",
    "convertImages": "yarn node --loader ts-node/esm utils/convertImages.ts",
    "lint:scripts": "eslint --ext .ts,.tsx,.astro . --max-warnings=0",
    "lint:styles": "stylelint \"src/**/*.{css,scss,html,astro}\"",
    "format": "prettier --write . --ignore-path .gitignore --ignore-unknown",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "postinstall": "husky"
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": "yarn lint:scripts",
    "*.{css,scss,html,astro}": "yarn lint:styles",
    "*": "yarn format"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/prefetch": "^0.4.0",
    "@astrojs/react": "^3.3.4",
    "@astrojs/sitemap": "^3.1.5",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource/noto-sans-jp": "^5.0.19",
    "@nanostores/react": "^0.7.2",
    "astro": "^4.6.4",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "auto-bind": "^5.0.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "deepmerge": "^4.3.1",
    "is-touch-device": "^1.0.1",
    "isomorphic-dompurify": "^2.7.0",
    "nanostores": "^0.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ress": "^5.0.2",
    "tailwindcss": "^3.4.4",
    "throttle-debounce": "^5.0.0",
    "typescript": "^5.4.5",
    "ua-parser-js": "^1.0.38"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/archiver": "^6.0.2",
    "@types/chokidar": "^2.1.3",
    "@types/is-touch-device": "^1.0.2",
    "@types/node": "^20.12.14",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/throttle-debounce": "^5.0.2",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.19",
    "chokidar": "^3.6.0",
    "cssnano": "^6.1.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-astro": "^0.34.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tailwindcss": "^3.15.2",
    "fs": "^0.0.2",
    "glob": "^10.3.16",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "path": "^0.12.7",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "sass": "^1.75.0",
    "sharp": "^0.33.4",
    "stylelint": "^16.3.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "svgo": "^3.2.0",
    "ts-node": "^10.9.2"
  },
  "packageManager": "yarn@4.1.1"
}
