{
  "name": "acty",
  "type": "module",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "archive": "yarn build && yarn node --no-warnings=ExperimentalWarning --loader ts-node/esm utils/genDistArchive.ts",
    "astro": "astro",
    "convertImages": "yarn node --no-warnings=ExperimentalWarning --loader ts-node/esm utils/convertImages.ts",
    "lint:scripts": "eslint --ext .ts,.tsx,.astro",
    "lint:styles": "stylelint \"src/**/*.{css,scss,html,astro}\"",
    "format": "prettier --write . --ignore-unknown",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": "yarn lint:scripts",
    "*.{css,scss,html,astro}": "yarn lint:styles",
    "*": "yarn format"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "4.0.0",
    "@astrojs/sitemap": "3.2.1",
    "@astrojs/tailwind": "^5.1.3",
    "@nanostores/react": "^0.8.2",
    "astro": "5.0.3",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "auto-bind": "^5.0.1",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "focus-visible": "^5.2.1",
    "image-size": "^1.1.1",
    "nanostores": "^0.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ress": "^5.0.2",
    "tailwindcss": "^3.4.16",
    "throttle-debounce": "^5.0.2",
    "typescript": "^5.7.2",
    "umaki": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/archiver": "^6.0.3",
    "@types/chokidar": "^2.1.7",
    "@types/js-beautify": "^1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@types/throttle-debounce": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.20",
    "chokidar": "^3.6.0",
    "cssnano": "^7.0.6",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "fs": "^0.0.2",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "js-beautify": "^1.15.1",
    "lint-staged": "^15.2.10",
    "path": "^0.12.7",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.82.0",
    "sharp": "^0.33.5",
    "stylelint": "^16.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "svgo": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "packageManager": "yarn@4.1.1"
}
