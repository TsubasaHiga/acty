{
  "name": "acty",
  "type": "module",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "astro dev & yarn convertImages --watch",
    "start": "astro dev",
    "build": "yarn convertImages && astro check && tsc --noEmit && astro build",
    "preview": "yarn build && astro preview",
    "archive": "yarn build && yarn node --loader ts-node/esm utils/genDistArchive.ts",
    "astro": "astro",
    "convertImages": "yarn node --loader ts-node/esm utils/convertImages.ts",
    "lint:scripts": "eslint --ext .ts,.tsx,.astro . --max-warnings=0",
    "lint:styles": "stylelint \"src/**/*.{css,scss,html,astro}\"",
    "format": "prettier --write . --ignore-path .gitignore --ignore-unknown",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": "yarn lint:scripts",
    "*.{css,scss,html,astro}": "yarn lint:styles",
    "*": "yarn format"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.0",
    "@astrojs/partytown": "^2.0.0",
    "@astrojs/prefetch": "^0.4.0",
    "@astrojs/react": "^3.0.2",
    "@astrojs/sitemap": "^3.0.0",
    "@astrojs/tailwind": "^5.0.0",
    "@fontsource/noto-sans-jp": "^5.0.10",
    "@nanostores/react": "^0.7.1",
    "astro": "^3.0.12",
    "astro-compress": "^2.0.15",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.0",
    "auto-bind": "^5.0.1",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "deepmerge": "^4.3.1",
    "is-touch-device": "^1.0.1",
    "isomorphic-dompurify": "^1.8.0",
    "nanostores": "^0.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ress": "^5.0.2",
    "tailwindcss": "^3.3.3",
    "throttle-debounce": "^5.0.0",
    "typescript": "^5.2.2",
    "ua-parser-js": "^1.0.35"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/archiver": "^5.3.2",
    "@types/chokidar": "^2.1.3",
    "@types/is-touch-device": "^1.0.0",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/throttle-debounce": "^5.0.0",
    "@types/ua-parser-js": "^0.7.37",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "archiver": "^6.0.1",
    "autoprefixer": "^10.4.15",
    "chokidar": "^3.6.0",
    "cssnano": "^6.0.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "fs": "^0.0.2",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "path": "^0.12.7",
    "postcss": "^8.4.29",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "svgo": "^3.2.0",
    "ts-node": "^10.9.1"
  }
}
