{
  "name": "acty",
  "type": "module",
  "version": "0.0.12",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "build-stg": "pnpm build && pnpm copyDistDir",
    "preview": "pnpm build && astro preview",
    "archive": "pnpm build && node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/genDistArchive.ts",
    "copyDistDir": "node --loader ts-node/esm scripts/copyDistDirectory.ts",
    "astro": "astro",
    "convertImages": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/convertImages.ts",
    "lint:scripts": "eslint --ext .ts,.tsx,.astro",
    "lint:styles": "stylelint \"src/**/*.{css,scss,html,astro}\"",
    "lint:html": "markuplint \"./src/**/*.astro\"",
    "format": "prettier --write . --ignore-unknown",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": "pnpm lint:scripts",
    "*.{css,scss,html,astro}": "pnpm lint:styles",
    "*": "pnpm format"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/react": "4.4.2",
    "@astrojs/sitemap": "3.6.0",
    "@nanostores/react": "^1.0.0",
    "astro": "5.16.6",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "auto-bind": "^5.0.1",
    "clsx": "^2.1.1",
    "focus-visible": "^5.2.1",
    "image-size": "^1.2.1",
    "jsdom": "^27.4.0",
    "nanostores": "^1.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ress": "^5.0.2",
    "typescript": "^5.9.3",
    "umaki": "^0.10.1"
  },
  "devDependencies": {
    "@markuplint/astro-parser": "^4.6.22",
    "@nanostores/logger": "^1.0.0",
    "@types/archiver": "^7.0.0",
    "@types/chokidar": "^2.1.7",
    "@types/js-beautify": "^1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "archiver": "^7.0.1",
    "astro-relative-links": "^0.4.2",
    "autoprefixer": "^10.4.23",
    "chokidar": "^3.6.0",
    "cssnano": "^7.1.2",
    "deepmerge": "^4.3.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fs": "^0.0.2",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "js-beautify": "^1.15.4",
    "lint-staged": "^16.2.7",
    "markuplint": "^4.14.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.97.1",
    "sharp": "^0.34.5",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.1",
    "svgo": "^4.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "packageManager": "pnpm@10.27.0"
}
