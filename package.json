{
  "name": "acty",
  "type": "module",
  "version": "0.0.10",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "build-stg": "yarn build && yarn copyDistDir",
    "preview": "astro preview",
    "archive": "yarn build && yarn node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/genDistArchive.ts",
    "copyDistDir": "yarn node --loader ts-node/esm scripts/copyDistDirectory.ts",
    "astro": "astro",
    "convertImages": "yarn node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/convertImages.ts",
    "lint:scripts": "eslint --ext .ts,.tsx,.astro",
    "lint:styles": "stylelint \"src/**/*.{css,scss,html,astro}\"",
    "lint:html": "markuplint \"./src/**/*.astro\"",
    "format": "prettier --write . --ignore-unknown",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": "yarn lint:scripts",
    "*.{css,scss,html,astro}": "yarn lint:styles",
    "*": "yarn format"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "4.2.2",
    "@astrojs/sitemap": "3.3.0",
    "@astrojs/tailwind": "^5.1.5",
    "@nanostores/react": "^0.8.4",
    "astro": "5.5.5",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "auto-bind": "^5.0.1",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "focus-visible": "^5.2.1",
    "image-size": "^1.2.0",
    "nanostores": "^0.11.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ress": "^5.0.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "umaki": "^0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@markuplint/astro-parser": "^4.6.18",
    "@nanostores/logger": "^0.4.0",
    "@types/archiver": "^6.0.3",
    "@types/chokidar": "^2.1.7",
    "@types/js-beautify": "^1",
    "@types/node": "^22.13.14",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "archiver": "^7.0.1",
    "astro-relative-links": "^0.4.2",
    "autoprefixer": "^10.4.21",
    "chokidar": "^3.6.0",
    "cssnano": "^7.0.6",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "fs": "^0.0.2",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "js-beautify": "^1.15.4",
    "lint-staged": "^15.5.0",
    "markuplint": "^4.11.8",
    "path": "^0.12.7",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.86.0",
    "sharp": "^0.33.5",
    "stylelint": "^16.17.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "svgo": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "packageManager": "yarn@4.1.1"
}
